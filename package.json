    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "cypress": "^12.13.0",
    "eslint": "^7.32.0",
    "husky": "^7.0.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^11.1.1",
    "start-server-and-test": "^2.0.0",
    "typescript": "~4.9.5"
  }
