{
  "name": "pet-proj",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --code-coverage",
    "cypress:run": "npx cypress run",
    "cypress:e2e": "start-test start http://localhost:4200 cypress:run"
